{
    "name": "orchestra/model",
    "description": "Model Component for Orchestra Platform",
    "homepage": "http://orchestraplatform.com/docs/latest/components/model",
    "keywords": ["orchestra-platform", "orchestral", "model", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Model\\" : "src/"
        }
    },
    "require": {
        "php": ">=5.6.0",
        "orchestra/contracts": "~3.3.2",
        "orchestra/notifier": "~3.3.0",
        "orchestra/support": "~3.3.1"
    },
    "require-dev": {
        "illuminate/auth": "~5.3.0",
        "illuminate/database": "~5.3.0",
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "~5.7"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.3-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
