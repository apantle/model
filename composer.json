{
    "name": "orchestra/model",
    "description": "Model Component for Orchestra Platform",
    "homepage": "http://orchestraplatform.com/docs/latest/components/model",
    "keywords": ["orchestra-platform", "orchestral", "model", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Model\\" : "src/"
        }
    },
    "require": {
        "php": ">=7.1",
        "orchestra/contracts": "~3.6.0",
        "orchestra/memory": "~3.6.0",
        "orchestra/support": "~3.6.0"
    },
    "require-dev": {
        "illuminate/auth": "~5.6.0",
        "illuminate/database": "~5.6.0",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.6-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
